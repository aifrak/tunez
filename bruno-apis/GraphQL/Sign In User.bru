meta {
  name: Sign In User
  type: graphql
  seq: 2
}

post {
  url: http://localhost:4000/gql
  body: graphql
  auth: none
}

headers {
  Content-Type: application/vnd.api+json
}

body:graphql {
  query {
    signInUser(email: "test10@example.com", password: "mypassword") {
      token
    }
  }
  
}

vars:post-response {
  token: response.body.data.signInUser.token
}

settings {
  encodeUrl: true
  timeout: 0
}
